  <style>
    {% set typoCSS %}

    .uppercase {
      text-transform: uppercase;
    }

    .headings {
      width: 100%;
      margin-left: auto;
      margin-right: auto;
      margin-top: 32px;
      padding: 16px;
    }

    .heading {
      position: relative;
    }

    .heading:before {
      content: attr(data-text);
      position: absolute;
      left: 0;
      top: -16px;
      line-height: 1;
      font-weight: 400;
      color: #333;
      font-size: 10px;
      font-family: sans-serif;
      text-transform: uppercase;
    }

    .paragraphs {
      width: 100%;
      max-width: 800px;
      margin-left: auto;
      margin-right: auto;
      margin-top: 32px;
      padding: 16px;
    }


    .glyphs {
      width: 100%;
      max-width: 800px;
      margin-left: auto;
      margin-right: auto;
      margin-top: 32px;
      display: flex;
      flex-wrap: wrap;
      flex-direction: row;
      justify-content: flex-start;
      align-items: flex-start;
      padding: 16px;
    }

    .glyph {
      width: 64px;
      height: 64px;
      line-height: 64px;
      text-align: center;
      flex: 0 0 64px;
      border: 1px solid #eee;
      margin: 0 -1px -1px 0;
      transition: all 0.25s;
    }

    .glyph:hover {
      background-color: #f4f4f4;
      box-shadow: inset 0 0 25px #ddd;
    }

    .darkMode .glyph {
      border: 1px solid #1d1d1d;
    }

    .darkMode .glyph:hover {
      border: 1px solid #1d1d1d;
      background-color: #080808;
      box-shadow: inset 0 0 25px #000;
    }

    {% endset %}
    {% css typoCSS %}
  </style>


